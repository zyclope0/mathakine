# Chemins relatifs pour compatibilit√© Codecov (CI vs local)
[run]
source =
    app
    server
relative_files = True

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
